@model IEnumerable<ContosoUniversityMvc.Models.SchoolViewModels.EnrollmentDateGroup>

@{
    ViewData["Title"] = "Student Body Statistics";
}

<h2>Student Body Statistics</h2>

<table class="table">
    <tr>
        <td>Enrollment Date</td>
        <td>Students</td>
    </tr>
    @foreach (var item in Model!)
    {
        <tr>
            <td>@Html.DisplayFor(modelItem => item.EnrollmentDate)</td>
            <td>@Html.DisplayFor(modelItem => item.StudentCount)</td>
        </tr>
    }
</table>